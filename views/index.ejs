<!doctype html>
<html>
  <head>
    <title>Socket.IO chat</title>
    <style>
      * { margin: 0; padding: 0; box-sizing: border-box; }
      body { font: 13px Helvetica, Arial; }
      form { background: #000; padding: 3px; position: fixed; bottom: 0; width: 100%; }
      form input { border: 0; padding: 10px; width: 90%; margin-right: .5%; }
      form button { width: 9%; background: rgb(130, 224, 255); border: none; padding: 10px; }
      #messages { list-style-type: none; margin: 0; padding: 0; }
      #messages li { padding: 5px 10px; }
      #messages li:nth-child(odd) { background: #eee; }
    </style>
  </head>
  <body>
    <ul id="messages"></ul>
    <form action="">
      <input id="m" autocomplete="off" /><button>Send</button>
    </form>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script>
        $(function () {
          var socket = io();
          socket.emit('getmessages', {userName: 'smith', token: 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyTmFtZSI6InNtaXRoIiwicGFzc3dvcmQiOiJzbWl0aDI0IiwiaWF0IjoxNTIzMDM0NDc0fQ.jmda6yXSIzZryLVxZB-KTBaEFClnhV4KZLseGTvi2Yo'});
          $('form').submit(function(){
            socket.emit('message', {senderName: 'smitesh', receiverName: 'smith',token: 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyTmFtZSI6InNtaXRlc2giLCJwYXNzd29yZCI6InNhd2FudCIsImlhdCI6MTUyMzA0NDE0Mn0.OXpSvAgmCjAdPSrOq_u8n8V3vRrvsHNxJFUPEFA1uXc', messageBody: $('#m').val(), timeSent: 213413});
            $('#m').val('');
            return false;
          });
          socket.on('message', function(msg){
            $('#messages').append($('<li>').text(msg));
          });
          socket.on('retrievedmessages',function(msg){
            console.log(msg);
            msg.forEach(element => {
              $('#messages').append($('<li>').text(element.messageBody));
            });

          });
        });
      </script>
  </body>
</html>